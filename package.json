{
  "name": "fullstack-starter",
  "version": "1.0.0",
  "description": "React, Redux, Typescript, Express, PostgreSQL w/ Prisma, and TailwindCSS.",
  "main": "built/index.js",
  "engines": {
    "node": "14.15.4",
    "yarn": "1.22.10",
    "npm": "7.5.2"
  },
  "scripts": {
    "build": "webpack --config ./bundler/webpack.prod.js",
    "start": "npm run build && npm run server",
    "client": "webpack serve --config ./bundler/webpack.dev.js --devtool inline-source-map --hot",
    "server": "tsc -p tsconfig.server.json && node built/",
    "dev": "concurrently \"nodemon\" \"npm run client\"",
    "server-dev": "nodemon"
  },
  "author": "Konrad Droeske",
  "license": "MIT",
