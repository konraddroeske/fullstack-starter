    "http-errors": "^1.8.0",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "tailwindcss": "^2.0.3",
    "webpack": "^5.24.1"
